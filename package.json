{
    "name": "my-app",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "run:server": "node ./src/index.js",
        "run:worker": "node ./src/index-worker.js",
        "start": "concurrently yarn:run:*",
        "start:prod": "pm2 start yarn start --name vendure-app",
        "stop:prod": "pm2 stop vendure-app",
        "migration:generate": "node migration generate",
        "migration:run": "node migration run",
        "migration:revert": "node migration revert"
    },
    "dependencies": {
        "@vendure/admin-ui-plugin": "1.1.3",
        "@vendure/asset-server-plugin": "1.1.3",
        "@vendure/core": "1.1.3",
        "@vendure/email-plugin": "1.1.3",
        "dotenv": "^10.0.0",
        "mysql": "2.18.1",
        "pm2": "^5.1.0"
    },
    "devDependencies": {
        "concurrently": "6.2.0"
    }
}
